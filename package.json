{
  "name": "react-power-path",
  "version": "1.1.2",
  "repository": "https://github.com/kthjm/react-power-path",
  "author": "kthjm <toxictoxer@gmail.com>",
  "license": "MIT",
  "main": "dist/react-power-path.cjs.js",
  "jsnext:main": "dist/react-power-path.es.js",
  "module": "dist/react-power-path.es.js",
  "description": "enhanced <path /> that recieve its totalLength",
  "keywords": ["react", "svg", "path", "animation"],
  "nyc": {
    "require": ["babel-register", "jsdom-global/register"],
    "include": ["src"],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "pretest": "flow",
    "test": "cross-env NODE_ENV=TEST nyc mocha",
    "prebuild": "yarn test",
    "build": "cross-env NODE_ENV=BUILD node rollup.config.all.js",
    "postbuild": "run-s process format copy",
    "process:cjs": "node dist/react-power-path.cjs.js",
    "process:min": "node dist/react-power-path.min.js",
    "process": "run-p process:cjs process:min",
    "format": "prettier --write **/*.js package.json",
    "copy": "flow-copy-source --verbose src dist",
    "coverage": "nyc report --reporter=lcov && codecov"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "flow-bin": "^0.57.3",
    "flow-copy-source": "^1.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.1.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^4.0.1",
    "uglify-es": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
